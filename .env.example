# Airflow Environment Configuration
# Copy this file to .env and fill in the actual values

# Airflow User ID (get with: id -u)
AIRFLOW_UID=502

# PostgreSQL Configuration
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_USER=airflow

# Slack Configuration
# Get your Slack Bot Token from https://api.slack.com/apps
SLACK_TOKEN=xoxb-your-slack-bot-token-here

# Vault Configuration (for development only)
# Generate a secure token for production use
VAULT_ROOT_TOKEN=your-vault-root-token
